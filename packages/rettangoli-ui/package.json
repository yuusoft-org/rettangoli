{
  "name": "@rettangoli/ui",
  "version": "0.1.31",
  "description": "A UI component library for building web interfaces.",
  "main": "dist/rettangoli-esm.min.js",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./src/index.js",
    "./cli": "./src/cli/index.js"
  },
  "module": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuusoft-org/rettangoli.git"
  },
  "author": {
    "name": "Luciano Hanyon Wu",
    "email": "han4wluc@yuusoft.com"
  },
  "license": "MIT",
  "scripts": {
    "check:contracts": "rtgl fe check",
    "build:dev": "rtgl fe build -d && bun run esbuild-dev.js",
    "build": "rtgl fe build && bun run esbuild.js",
    "vt:generate": "bun run build:dev && rtgl vt generate --skip-screenshots",
    "vt:docker": "bun run build:dev && docker run --rm --user $(id -u):$(id -g) -v \"$PWD:/app\" -w /app han4wluc/rtgl:playwright-v1.57.0-rtgl-v0.0.38 rtgl vt generate",
    "vt:report": "bun run vt:docker && rtgl vt report",
    "vt:accept": "rtgl vt accept",
    "serve": "bunx serve .rettangoli/vt/_site"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "looks-same": "^9.0.1",
    "pixelmatch": "^6.0.0",
    "playwright": "1.57.0",
    "pngjs": "^7.0.0"
  },
  "keywords": [
    "ui",
    "library",
    "web",
    "components"
  ],
  "bugs": {
    "url": "https://github.com/yuusoft-org/rettangoli/issues"
  },
  "homepage": "https://github.com/yuusoft-org/rettangoli#readme",
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@rettangoli/fe": "workspace:*",
    "commander": "^13.1.0",
    "jempl": "1.0.0",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.21.0",
    "snabbdom": "^3.6.2"
  }
}
